// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model PitList {
  id           Int       @id @default(autoincrement())
  signed       String
  event        String
  match        String
  checks       String
  submitted_at DateTime?
}
